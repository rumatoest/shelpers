					"to":[{"key_code": "up_arrow","modifiers":["left_shift", "left_command"]}],
					"conditions": [{"type": "frontmost_application_if", "bundle_identifiers":["^com\\.jetbrains\\.*"]}]
				},
				{
					"type": "basic",
